{
  "name": "desktop-electron-webpack",
  "productName": "Bestcycling TV",
  "version": "4.0.4",
  "description": "Aplicaci√≥n de escritorio Bestcycling desarrollada con electron",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env-shell ENV=dev electron-forge start",
    "dev": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge start",
    "package": "electron-forge package",
    "make": "cross-env-shell ENV=prod electron-forge make --arch=universal",
    "make:macos": "cross-env-shell ENV=prod electron-forge make --arch=universal",
    "make:macos:dev": "cross-env-shell ENV=dev electron-forge make ",
    "make:macos:devprod": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge make",
    "make:win:devprod": "npm run make:win:devprod:x64 && make:win:devprod:ia32",
    "make:win:devprod:x64": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge make --arch=x64",
    "make:win:devprod:ia32": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge make --arch=ia32",
    "push:macos": "cross-env-shell ENV=prod electron-forge publish --arch=universal",
    "push:win": "npm run push:win:x64 && npm run push:win:ia32",
    "push:win:x64": "cross-env-shell ENV=prod electron-forge publish --arch=x64",
    "push:win:ia32": "cross-env-shell ENV=prod electron-forge publish --arch=ia32",
    "push:macos:devprod": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge publish --arch=universal",
    "push:win:devprod": "npm run push:win:devprod:x64 && npm run push:win:devprod:ia32",
    "push:win:devprod:ia32": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge publish --arch=ia32",
    "push:win:devprod:x64": "cross-env-shell ENV=prod FLAVOUR=2 electron-forge publish --arch=x64",
    "publish": "node process/gen_manifest --channel=qiqe-temp",
    "publish:beta": "node process/gen_manifest --channel=beta",
    "publish:revision": "node process/gen_manifest --channel=revision",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest"
  },
  "keywords": [],
  "author": {
    "name": "Bestcycling",
    "email": "info@bestcycling.es"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0",
    "@electron-forge/maker-dmg": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0",
    "@electron-forge/maker-zip": "^6.0.0",
    "@electron-forge/plugin-webpack": "^6.0.0",
    "@electron-forge/publisher-s3": "^6.0.0",
    "@electron/universal": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "aws-sdk": "^2.1248.0",
    "bufferutil": "^4.0.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^21.2.0",
    "eslint": "^8.27.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.2.2",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.3.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "~4.8.4",
    "utf-8-validate": "^5.0.10"
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-15",
    "@commonify/lowdb": "^3.0.0",
    "@types/follow-redirects": "^1.14.1",
    "@types/fs-extra": "^9.0.13",
    "async-lock": "^1.4.0",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "follow-redirects": "^1.15.2",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "kitsu": "^10.0.2"
  }
}
